        {{#if:{{{Production|}}}|{{#if:{{{Producer|}}}|<br />&nbsp;}}[[{{StaffCorrection|{{{Production}}}}}|{{{Production}}}]]}}
        {{#if:{{{Production1_1|}}}|
         {{#if:{{{Producer|}}}{{{Production|}}}|<br />&nbsp;}}
         {{#invoke:Infobox/Comic|Contributors
          | Contributors.min = 1
          | Contributors.max = 5
          | Contributors.prefix = Production1_
          | Contributors.between = <br />}}
